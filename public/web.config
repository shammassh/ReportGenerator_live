<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="ReverseProxyToNode" stopProcessing="true">
                    <match url="(.*)" />
                    <action type="Rewrite" url="http://localhost:3003/{R:1}" />
                </rule>
            </rules>
        </rewrite>
        <proxy timeout="00:02:00" />
    </system.webServer>
</configuration>
